#%RAML 1.0
title: Vehicles API

types:
  Bicycle: !include types/bicycle.raml
  Boat: !include types/boat.raml
  Car: !include types/car.raml
  Plane: !include types/plane.raml
  
/vehicles:
  post:
    body:
      application/json:
        type: Bicycle | Boat | Car | Plane
  get:
    responses:
      200:
        body:
          application/json:
            type: (Bicycle | Boat | Car | Plane)[]
